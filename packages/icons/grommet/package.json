{
  "name": "@hpe-design/icons-grommet",
  "description": "HPE Icons for use with Grommet and the HPE Design System.",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "repository": "https://github.com/grommet/hpe-design-system/tree/master/packages/icons/grommet",
  "type": "module",
  "types": "./dist/hpe-icons.d.ts",
  "exports": {
    ".": {
      "types": "./dist/hpe-icons.d.ts",
      "import": "./dist/hpe-icons.js",
      "require": "./dist/hpe-icons.cjs"
    }
  },
  "files": [
    "dist",
    "package.json",
    "README.md",
    "CHANGELOG.md",
    "COPYRIGHT.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc -b && vite build && npm run copy-types",
    "copy-types": "cp src/js/icons/index.d.ts dist/hpe-icons.d.ts",
    "lint": "eslint .",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "styled-components": "^6.0.0 || ^7.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/react": "^8.6.14",
    "@storybook/react-vite": "^8.6.14",
    "@storybook/test": "^8.6.14",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "grommet-theme-hpe": "^7.0.0",
    "minimatch": "^10.0.3",
    "storybook": "^8.6.14",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2"
  }
}
