{
  "name": "@hpe-design/icons-svg",
  "description": "HPE Icons in SVG format for use with the HPE Design System.",
  "version": "0.2.0",
  "license": "Apache-2.0",
  "repository": "https://github.com/grommet/hpe-design-system/tree/master/packages/icons-svg",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/hpe-icons.js",
      "require": "./dist/hpe-icons.cjs"
    },
    "./*.svg": "./dist/*.svg"
  },
  "files": [
    "dist",
    "package.json",
    "README.md",
    "CHANGELOG.md",
    "COPYRIGHT.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc && vite build",
    "test": "vitest --watch=false"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "typescript": "catalog:",
    "vite": "^7.2.2",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-static-copy": "^2.3.2",
    "vitest": "^3.0.9"
  }
}
