      "$description": "",
      "$extensions": {
        "com.figma": {
          "hiddenFromPublishing": true,
          "scopes": ["WIDTH_HEIGHT"],
          "codeSyntax": {}
        }
      }
    }
  },
  "fontWeight": {
    "thin": {
