      "$description": "Thin is rarely, if at all, used.",
      "$extensions": {
        "com.figma": {
          "hiddenFromPublishing": false,
          "scopes": ["FONT_STYLE"],
          "codeSyntax": {}
        }
      }
    },
    "light": {
      "$type": "number",
      "$value": 300,
